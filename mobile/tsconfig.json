{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["react", "react-native"],
    "baseUrl": "./",
    "paths": {
      "@/src/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
