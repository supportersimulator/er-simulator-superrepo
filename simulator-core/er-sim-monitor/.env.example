# Dotenv Configuration
DOTENV_CONFIG_SILENT=true

# Google OAuth Client (from Google Cloud Console)
GOOGLE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your_client_secret_here

# Google Sheet Info (Master Scenario Convert Sheet)
SHEET_ID=your_spreadsheet_id_here
GOOGLE_SHEET_ID=your_spreadsheet_id_here
TOKEN_PATH=./config/token.json

# Apps Script Info (Sim Builder Production Project)
APPS_SCRIPT_ID=your_apps_script_project_id_here

# Apps Script Deployment Info
DEPLOYMENT_ID=your_deployment_id_here
WEB_APP_URL=https://script.google.com/macros/s/your_deployment_id_here/exec

# OpenAI API Key (for scenario processing)
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# ATSR Standalone Project (Title Generator)
ATSR_SCRIPT_ID=your_atsr_script_id_here

# Live Sync Server (optional)
LIVE_SYNC_PORT=3333
